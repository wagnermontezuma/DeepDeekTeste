{
  "name": "deepseek-assistant",
  "version": "1.0.0",
  "description": "Interface de chat para DeepSeek API",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "tsc && vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.28",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "vite": "^5.4.17"
  }
}
